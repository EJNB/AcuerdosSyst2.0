{% extends "::base.html.twig" %}

{% block stylesheets %}
     {#<link href="{{ asset('bootstrap/css/bootstrap-theme.min.css') }}" rel="stylesheet" media="screen">#}
     <link href="{{ asset('bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" media="screen">
     <link href="{{ asset('jQueryUI/jquery-ui.min.css') }}" rel="stylesheet" media="screen">
     <link href="{{ asset('admin/styles.css') }}" rel="stylesheet" media="screen">
     <link href="{{ asset('Font-Awesome/css/font-awesome.min.css') }}" rel="stylesheet" media="all">
     <link href="{{ asset('datetimepicker/build/css/bootstrap-datetimepicker.min.css') }}" rel="stylesheet" media="screen">
     <link href="{{ asset('plugins/PNotify/pnotify.custom.min.css') }}" rel="stylesheet" media="all">
     <link href="{{ asset('plugins/bootstrap-select-1.12.0/dist/css/bootstrap-select.min.css') }}" rel="stylesheet" media="all">
{% endblock %}

{% block body %}

    <header>
        {% block header %}
            {% include 'layout-admin-header.html.twig' %}
        {% endblock %}
    </header>

    <article id="content" class="content">
        {% block content %}
        {% endblock %}
    </article>

    <footer>
        {% block footer %}
            {#{% include 'layout-admin-footer.html.twig' %}#}
        {% endblock %}
    </footer>

{% endblock %}

{% block javascripts %}
    <script src="{{ asset('jquery/jquery-1.11.1.min.js') }}"></script>
    <script src="{{ asset('jQueryUI/jquery-ui.min.js') }}"></script>
    <script src="{{ asset('bootstrap/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('moment/min/moment.min.js') }}"></script>
    <script src="{{ asset('moment/min/locales.min.js') }}"></script>
    <script src="{{ asset('datetimepicker/build/js/bootstrap-datetimepicker.min.js') }}"></script>
    <script src="{{ asset('plugins/PNotify/pnotify.custom.min.js') }}"></script>
    <script src="{{ asset('plugins/bootstrap-select-1.12.0/dist/js/bootstrap-select.min.js') }}"></script>
    <script src="{{ asset('admin/scripts.js') }}"></script>
{% endblock %}