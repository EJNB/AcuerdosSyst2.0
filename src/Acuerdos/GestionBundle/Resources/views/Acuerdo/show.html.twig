{% extends '::layout-admin.html.twig' %}

{% block body -%}
    <h1>Acuerdo</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Idacuerdo</th>
                <td>{{ entity.idAcuerdo }}</td>
            </tr>
            <tr>
                <th>Fechacreacion</th>
                <td>{{ entity.fechaCreacion|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>
                <th>Fechacumplimientoindicada</th>
                <td>{{ entity.fechaCumplimientoIndicada|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Fechacumplimientoreal</th>
                <td>{{ entity.fechaCumplimientoReal|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('acuerdo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('acuerdo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    {#<li>{{ form(delete_form) }}</li>#}
</ul>
{% endblock %}
